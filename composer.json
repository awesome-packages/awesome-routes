{
  "name": "rhuangabrielsantos/simple-routes",
  "description": "A simple library for handling routes",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SimpleRoutes\\": "src/"
    }
  },
  "autoload-dev": {
